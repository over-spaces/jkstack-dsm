<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.jkstack.dsm.user.entity.UserEntity">
    <select id="pageUsersByDepartmentId" parameterType="string" resultType="com.jkstack.dsm.user.entity.UserEntity">
        select u.* from dsm_user u left join dsm_user_department d on u.user_id=d.user_id where d.department_id=${departmentId};

    </select>

</mapper>